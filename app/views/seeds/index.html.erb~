<h1>Listing seeds</h1>

<table>
  <thead>
    <tr>
      <th>Variety</th>
      <th>Seed number</th>
      <th>Harvest days</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seeds.each do |seed| %>
      <tr>
        <td><%= seed.variety_id %></td>
        <td><%= seed.variety.name %></td>
        <td><%= seed.seed_number %></td>
        <td><%= seed.harvest_days %></td>
        <td><%= link_to 'Show', seed %></td>
        <td><%= link_to 'Edit', edit_seed_path(seed) %></td>
        <td><%= link_to 'Destroy', seed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

